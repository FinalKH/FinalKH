<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="travel">

	<select id="selectAllArea" resultType="com.kh.web.model.travel.dto.AreaVO">
		SELECT * FROM T_Area
	</select>

	<select id="selectAllSiGunGu" resultType="com.kh.web.model.travel.dto.SiGunGuVO">
		SELECT * FROM T_SiGunGu
	</select>
	
	<select id="selectAllContentCommon" resultType="com.kh.web.model.travel.dto.ContentCommonVO">
		SELECT * FROM T_ContentCommon
	</select>
	<select id="bringAllInMap" resultType="com.kh.web.model.travel.dto.ContentCommonVO">
		SELECT * FROM T_ContentCommon where (mapX between #{westBP} AND #{eastBP}) AND (mapY between #{southBP} AND #{northBP}) AND rownum &lt;= 10
	</select>
</mapper>
